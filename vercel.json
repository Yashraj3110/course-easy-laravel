{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public/build"
            }
        },
        {
            "src": "public/index.php" }
    ],
    "routes": [{ "src": "/(.*)", "dest": "public/index.php" }]
}
